@page "/diceview"

<h1>Dice View</h1>

<h3>Current dice roll: @LatestRoll</h3>

<h3>Latest 5 rolls: @string.Join(',', LatestFiveRolls.Select(roll => roll))</h3>

<label for="low">Low</label>
<input id="low" type="number" @bind="@Low">

<label for="high">Low</label>
<input id="high" type="number" @bind="@High">

<button type="button" @onclick="RollDice">Roll dice</button>





